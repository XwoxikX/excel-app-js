{
  "parser": "babel-eslint",
  "env": {
	"es6": true,
	"browser": true,
	"node": true
  },
  "rules": {
	"semi": "off",
	"arrow-parens": "off",
	"indent": "off",
	"comma-dangle": "off",
	"no-tabs": "off",
	"require-jsdoc": "off",
	"operator-linebreak": "off",
	"no-mixed-spaces-and-tabs": "off",
	"max-len": "off",
	"space-before-blocks": "off",
	"no-trailing-spaces": "off",
	"no-unused-vars": "off"
  },
  "extends": ["eslint:recommended", "google"]
}